        <obo:IAO_0000115 xml:lang="pt">Uma fotografia de uma amostra de mão.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">hand sample image</rdfs:label>
        <rdfs:label xml:lang="pt">imagem de amostra de mão</rdfs:label>
    </owl:Class>
    


