        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_contour_map"/>
        <obo:IAO_0000115 xml:lang="en">A map that represents the increase of temperature with depth. It usually refers to depths below 200 m.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa que representa o aumento de temperatura com profundidade. Geralmente se refere a profundidades abaixo de 200 m.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Dictionary of Earth Sciences, Michael Allaby</obo:IAO_0000119>
        <rdfs:label xml:lang="en">geothermal gradient map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de gradiente geotérmico</rdfs:label>
    </owl:Class>
    


