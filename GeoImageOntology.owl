    <!-- http://purl.obolibrary.org/obo/IAO_0000038 -->

    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000038">
        <obo:IAO_0000115 xml:lang="pt">Um diagrama que apresenta uma ou mais tuplas de informação através do mapeamento de tais tuplas em um espaço bidimensional de maneira arbitrária.</obo:IAO_0000115>
        <rdfs:label xml:lang="pt">gráfico</rdfs:label>
    </rdf:Description>
    


    <!-- http://purl.obolibrary.org/obo/IAO_0000101 -->

    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000101">
        <rdfs:label xml:lang="pt">imagem</rdfs:label>
    </rdf:Description>
    


    <!-- http://purl.obolibrary.org/obo/IAO_0000179 -->

    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000179">
        <obo:IAO_0000115 xml:lang="pt">Um histograma é a representação gráfica em colunas ou em barras (retângulos) de um conjunto de dados previamente tabulado e dividido em classes uniformes ou não uniformes.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="pt">WEB: https://pt.wikipedia.org/wiki/Histograma</obo:IAO_0000119>
        <rdfs:label xml:lang="pt">histograma</rdfs:label>
    </rdf:Description>
    


    <!-- http://purl.obolibrary.org/obo/IAO_0000185 -->

    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000185">
        <rdfs:label xml:lang="pt">fotografia</rdfs:label>
    </rdf:Description>
    


    <!-- http://purl.obolibrary.org/obo/IAO_0000309 -->

    <rdf:Description rdf:about="http://purl.obolibrary.org/obo/IAO_0000309">
        <obo:IAO_0000115 xml:lang="pt">Uma figura que expressa uma ou mais proposições.</obo:IAO_0000115>
        <rdfs:label xml:lang="pt">diagrama</rdfs:label>
    </rdf:Description>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_3d_block_diagram -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_3d_block_diagram">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <obo:IAO_0000115 xml:lang="en">A representation of an imaginary rectangular block of the earth&apos;s crust (representing geological and topographic features) in what appears to be a three-dimensional perspective, showing a surface area at the top and including one or more (usually two) vertical cross sections.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Uma representação de um bloco retangular imaginário da crosta terrestre (representando características geológicas e topográficas) no que parece ser uma perspectiva tridimensional, mostrando uma área de superfície no topo e incluindo uma ou mais (geralmente duas) seções transversais verticais.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Glossary of Geology. Klaus K.E Neundorf, James P. Mehl Jr., Julia A. Jackson</obo:IAO_0000119>
        <rdfs:label xml:lang="en">3d block diagram</rdfs:label>
        <rdfs:label xml:lang="pt">bloco diagrama</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_Photomicrograph -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_Photomicrograph">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000185"/>
        <owl:disjointWith rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_satellite_image"/>
        <obo:IAO_0000115 xml:lang="en">A photograph taken through a microscope or similar device to show a magnified image of an object.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Uma fotografia obtida através de um microscópio ou dispositivo similar para mostrar uma visão aumentada de um objeto.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Micrograph</obo:IAO_0000119>
        <rdfs:label xml:lang="pt">microfotografia</rdfs:label>
        <rdfs:label xml:lang="en">photomicrograph</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_aerial_photography -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_aerial_photography">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000185"/>
        <obo:IAO_0000115 xml:lang="pt">fotografia a partir de uma aeronave ou outro objeto voador.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="en">photograph from an aircraft or other flying object.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Aerial_photography</obo:IAO_0000119>
        <rdfs:label xml:lang="en">aerial photography</rdfs:label>
        <rdfs:label xml:lang="pt">fotografia aérea</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_altimetric_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_altimetric_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_topographic_map"/>
        <owl:disjointWith rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_bathymetric_map"/>
        <obo:IAO_0000115 xml:lang="en">a contour map with heights indicated by outlines (isobaths) drawn at regular intervals.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">um mapa de contorno com alturas indicadas por contornos(isóbatas) traçados em intervalos regulares.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">altimetric map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa altimétrico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_amplitute_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_amplitute_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_contour_map"/>
        <obo:IAO_0000115 xml:lang="en">A display of amplitude measurements on a slice on the horizon.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Uma exibição de medições de amplitude em uma fatia no horizonte.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Glossary of Geology. Klaus K.E Neundorf, James P. Mehl Jr., Julia A. Jackson</obo:IAO_0000119>
        <rdfs:label xml:lang="en">amplitude map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de amplitude</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_bathymetric_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_bathymetric_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_topographic_map"/>
        <obo:IAO_0000115 xml:lang="en">A contour map of the bottom of a body of water (such as the seabed) with depths indicated by outlines (isobaths) drawn at regular intervals.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">um mapa de contorno do fundo de um corpo de água (como o fundo do mar) com profundidades indicadas por contornos(isóbatas) traçados em intervalos regulares.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Glossary of Geology, Klaus K. E. Neuendorf, James P. Mehl Jr. and Julia A. Jackson</obo:IAO_0000119>
        <rdfs:label xml:lang="en">bathymetric map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa batimétrico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_bouguer_anomaly_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_bouguer_anomaly_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_choropleth_map"/>
        <obo:IAO_0000115 xml:lang="en">Map that shows a measure of gravity that is correct for topography (elevation) and latitude, but not for isostasis (subsurface rock density).</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Mapa que mostra uma medida de gravidade que é correta para a topografia(elevação) e latitude, mas não para a isostasia (densidade de rochas subsuperficiais).</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Dictionary of Petroleum Exploration, Drilling, &amp; Production, Norman J. Hyne</obo:IAO_0000119>
        <rdfs:label xml:lang="en">bourguer anomaly map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de anomalia bouguer</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_caliper_profile -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_caliper_profile">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_wireline_well_log"/>
        <obo:IAO_0000115 xml:lang="pt">O registro produzido por um paquímetro carregado por mola que se ajusta continuamente ao tamanho de um poço quando ele é puxado para a superfície. O perfil é uma medida direta do diâmetro do poço em função da profundidade.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="en">The record produced by a spring-loaded caliper that continually adjusts itself to the size of a borehole as it is pulled to the surface. The log is a direct measure of the diameter of the borehole as a function of depth.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Dictionary of Earth Sciences, Michael Allaby</obo:IAO_0000119>
        <rdfs:label xml:lang="en">caliper profile</rdfs:label>
        <rdfs:label xml:lang="pt">perfil caliper</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_cartogram -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_cartogram">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_thematic_map"/>
        <obo:IAO_0000115 xml:lang="en">A map that purposely distorts geographic space based on values of a theme.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa que distorce propositalmente o espaço geográfico com base nos valores de um tema.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Thematic_map</obo:IAO_0000119>
        <rdfs:label xml:lang="en">cartogram</rdfs:label>
        <rdfs:label xml:lang="pt">cartograma</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_choropleth_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_choropleth_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_thematic_map"/>
        <obo:IAO_0000115 xml:lang="en">A thematic map in which areas are shaded or patterned in proportion to the measurement of the statistical variable being displayed on the map.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa temático no qual as áreas são sombreadas ou padronizadas na proporção da medida da variável estatística exibida no mapa.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Choropleth_map</obo:IAO_0000119>
        <rdfs:label xml:lang="en">choropleth map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa coroplético</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_chronostratigraphic_chart -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_chronostratigraphic_chart">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000308"/>
        <obo:IAO_0000115 xml:lang="pt">Exposição gráfica, com tempo geológico ao longo do eixo vertical e distância referente a uma seção de referência no eixo horizontal, para demonstrar idades relativas e extensão geográfica de estratos ou unidades estratigráficas de uma determinada área.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="en">Graphical display with geological time along the vertical axis and distance from a reference section on the horizontal axis to demonstrate relative ages and geographic extent of strata or stratigraphic units within a given area.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="pt">http://dicionariodopetroleo.com.br/</obo:IAO_0000119>
        <rdfs:label xml:lang="pt">carta cronoestratigráfica</rdfs:label>
        <rdfs:label xml:lang="en">chronostratigraphic chart</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_compositional_diagram -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_compositional_diagram">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000309"/>
        <obo:IAO_0000115 xml:lang="en">diagram representing the spatial distribution of mineral composition in a rock sample.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">diagrama que representa a distribuição espacial da composição mineral  em  uma amostra de rocha.</obo:IAO_0000115>
        <obo:IAO_0000118 xml:lang="pt">mapa composicional de amostra</obo:IAO_0000118>
        <rdfs:label xml:lang="en">compositional diagram</rdfs:label>
        <rdfs:label xml:lang="pt">diagrama composicional</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_compositional_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_compositional_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_choropleth_map"/>
        <obo:IAO_0000115 xml:lang="en">a map that represents the geographical distribution of mineral compositions.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">mapa que representa a distribuição geográfica de composições minerais.</obo:IAO_0000115>
        <rdfs:label xml:lang="en">compositional map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa composicional</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_contour_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_contour_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_thematic_map"/>
        <obo:IAO_0000115 xml:lang="en">A map that depicts continuous phenomena. Each line-bounded area on this type of map represents a region with the same value.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa que descreve fenômenos contínuos. Cada área delimitada por linhas neste tipo de mapa representa uma região com o mesmo valor.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Thematic_map</obo:IAO_0000119>
        <rdfs:label xml:lang="en">contour map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de contorno</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_cross_section -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_cross_section">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000308"/>
        <obo:IAO_0000115 xml:lang="en">A cross section of a diagram or drawing showing features cut by a particular specific plane. An actual exhibit or cut showing cut geological features.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Corte transversal a um diagrama ou desenho que mostra características cortadas por um determinado plano específico.  Uma exposição ou corte real que mostra recursos geológicos cortados.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Glossary of Geology. Klaus K.E Neundorf, James P. Mehl Jr., Julia A. Jackson</obo:IAO_0000119>
        <rdfs:label xml:lang="en">cross section</rdfs:label>
        <rdfs:label xml:lang="pt">seção transversal</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_dasymetric_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_dasymetric_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_thematic_map"/>
        <obo:IAO_0000115 xml:lang="en">A map which uses areal symbols to spatially classify volumetric data.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa que usa símbolos de área para classificar espacialmente dados volumétricos.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Dasymetric_map</obo:IAO_0000119>
        <rdfs:label xml:lang="en">dasymetric map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa dasimétrico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_density_profile -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_density_profile">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_wireline_well_log"/>
        <obo:IAO_0000115 xml:lang="en">A record of the density of the rocks penetrated by a drill hole.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um registro da densidade das rochas perfuradas por uma broca.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="pt">Dictionary of Earth Sciences, Michael Allaby</obo:IAO_0000119>
        <rdfs:label xml:lang="en">density profile</rdfs:label>
        <rdfs:label xml:lang="pt">perfil de densidade</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_dot_distribution_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_dot_distribution_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_thematic_map"/>
        <obo:IAO_0000115 xml:lang="en">A map that uses a dot symbol to show the presence of a feature or a phenomenon. Dot maps rely on a visual scatter to show spatial pattern.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa que usa um símbolo de ponto para mostrar a presença de um elemento ou fenômeno. Os mapas de pontos contam com uma dispersão visual para mostrar o padrão espacial.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Dot_distribution_map</obo:IAO_0000119>
        <rdfs:label xml:lang="en">dot distribution map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de distribuição de pontos</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_facies_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_facies_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_dasymetric_map"/>
        <obo:IAO_0000115 xml:lang="en">A map showing the distribution of different types of rock attributes or facies occurring within a deisgnated geologic unit.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa mostrando a distribuição de diferentes tipos de atributos ou fácies rochosas que ocorrem dentro de uma unidade geológica demarcada.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://www.mindat.org/glossary/facies_map

Mindat.org glossary, project of the Hudson Institute of Mineralogy.</obo:IAO_0000119>
        <rdfs:label xml:lang="en">facies map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de fácies</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_flow_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_flow_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_thematic_map"/>
        <obo:IAO_0000115 xml:lang="en">A map that uses lines or arrows to portray movement between two or more destinations. These lines or arrows can also represent quantities of things being transferred between locations. This can be done via use of proportional symbols.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa que usa linhas ou setas para retratar o movimento entre dois ou mais destinos. Essas linhas ou setas também podem representar quantidades de coisas sendo transferidas entre locais. Isso pode ser feito através do uso de símbolos proporcionais.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">https://en.wikipedia.org/wiki/Thematic_map</obo:IAO_0000119>
        <rdfs:label xml:lang="en">flow map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa de fluxo</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_gamma_ray_profile -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_gamma_ray_profile">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_wireline_well_log"/>
        <obo:IAO_0000115 xml:lang="en">A measurement of the natural radioactivity of the rocks through which the drill hole passes.  The record is expressed in API units</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Uma medida da radioatividade natural das rochas através das quais o furo passa. O registro é expresso em unidades de API</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="pt">Dictionary of Earth Sciences, Michael Allaby</obo:IAO_0000119>
        <rdfs:label xml:lang="en">gamma ray profile</rdfs:label>
        <rdfs:label xml:lang="pt">perfil de raio gama</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geological_cross_section -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geological_cross_section">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_cross_section"/>
        <obo:IAO_0000115 xml:lang="en">An interpretation of a vertical section through the Earth’s surface, for which evidence was obtained by geologic and geophysical techniques or from a geologic map.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Uma interpretação de uma seção vertical através da superfície da Terra, para o qual foram obtidas evidências por técnicas geológicas e geofísicas ou a partir de um mapa geológico.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Dictionary of Earth Science, Michael Allaby</obo:IAO_0000119>
        <rdfs:label xml:lang="en">geological cross section</rdfs:label>
        <rdfs:label xml:lang="pt">seção geológica</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geological_map -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geological_map">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_choropleth_map"/>
        <obo:IAO_0000115 xml:lang="en">A map in which geological information is recorded, such as the distribution, nature and age relationships of rock units (surface deposits may or may not be mapped separately), and the occurrence of structural features (folds, faults, joints), mineral deposits and locations. fossils</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Um mapa no qual são registradas informações geológicas, como a distribuição, natureza e relações de idade das unidades rochosas (depósitos superficiais podem ou não ser mapeados separadamente), e a ocorrência de características estruturais (dobras, falhas, juntas), depósitos minerais e localidades fósseis</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Glossary of Geology. Klaus K.E Neundorf, James P. Mehl Jr., Julia A. Jackson</obo:IAO_0000119>
        <rdfs:label xml:lang="en">geological map</rdfs:label>
        <rdfs:label xml:lang="pt">mapa geológico</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geophysical_cross_section -->

    <owl:Class rdf:about="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geophysical_cross_section">
        <rdfs:subClassOf rdf:resource="http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_cross_section"/>
        <obo:IAO_0000115 xml:lang="en">A cross section showing the variation of one property, such as elevation or gravity, usually with cordinates, relative to another property, such as distance.</obo:IAO_0000115>
        <obo:IAO_0000115 xml:lang="pt">Uma seção transversal que mostra a variação de uma propriedade, como elevação ou gravidade, geralmente com coordenadas, em relação a outra propriedade, como distância.</obo:IAO_0000115>
        <obo:IAO_0000119 xml:lang="en">Glossary of Geology. Klaus K.E Neundorf, James P. Mehl Jr., Julia A. Jackson</obo:IAO_0000119>
        <rdfs:label xml:lang="en">geophysical cross section</rdfs:label>
        <rdfs:label xml:lang="pt">seção geofísica</rdfs:label>
    </owl:Class>
    


    <!-- http://www.inf.ufrgs.br/bdi/ontologies/GeoImageOntology#UFRGS:GeoImage_geothermal_gradient_map -->

